[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"JOEL1:4","chapter":1,"book":"Joel","verse":4,"readable":"Joel 1:4"}},{"type":"char","content":"What the gnawing locust has left, the swarming locust has eaten;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And what the swarming locust has left, the creeping locust has eaten;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And what the creeping locust has left, the stripping locust has eaten."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:4","chapter":1,"book":"Joel","verse":4,"readable":"Joel 1:4"}},{"type":"verse","reference":{"id":"JOEL1:5","chapter":1,"book":"Joel","verse":5,"readable":"Joel 1:5"}},{"type":"char","content":"Awake, you heavy drinkers, and weep;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And wail, all you wine drinkers,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Because of the sweet wine,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"For it has been eliminated from your mouth."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:5","chapter":1,"book":"Joel","verse":5,"readable":"Joel 1:5"}},{"type":"verse","reference":{"id":"JOEL1:6","chapter":1,"book":"Joel","verse":6,"readable":"Joel 1:6"}},{"type":"char","content":"For a nation has invaded my land,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Mighty and without number;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Its teeth are the teeth of a lion,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And it has the jaws of a lioness."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:6","chapter":1,"book":"Joel","verse":6,"readable":"Joel 1:6"}},{"type":"verse","reference":{"id":"JOEL1:7","chapter":1,"book":"Joel","verse":7,"readable":"Joel 1:7"}},{"type":"char","content":"It has made my vine a waste"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And my fig tree a stump."}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"It has stripped them bare and hurled them away;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Their branches have become white."}]},{"type":"poetry_line","indent":1,"children":[]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:7","chapter":1,"book":"Joel","verse":7,"readable":"Joel 1:7"}},{"type":"verse","reference":{"id":"JOEL1:8","chapter":1,"book":"Joel","verse":8,"readable":"Joel 1:8"}},{"type":"char","content":"Wail like a virgin clothed with sackcloth"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"For the groom of her youth."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:8","chapter":1,"book":"Joel","verse":8,"readable":"Joel 1:8"}},{"type":"verse","reference":{"id":"JOEL1:9","chapter":1,"book":"Joel","verse":9,"readable":"Joel 1:9"}},{"type":"char","content":"The grain offering and the drink offering have been cut off"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"From the house of the Lord."}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"The priests mourn,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"The ministers of the Lord."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:9","chapter":1,"book":"Joel","verse":9,"readable":"Joel 1:9"}},{"type":"verse","reference":{"id":"JOEL1:10","chapter":1,"book":"Joel","verse":10,"readable":"Joel 1:10"}},{"type":"char","content":"The field is ruined,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"The land mourns;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"For the grain is ruined,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"The new wine has dried up,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Fresh oil has failed."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:10","chapter":1,"book":"Joel","verse":10,"readable":"Joel 1:10"}},{"type":"verse","reference":{"id":"JOEL1:11","chapter":1,"book":"Joel","verse":11,"readable":"Joel 1:11"}},{"type":"char","content":"Be ashamed, you farm workers,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Wail, you vinedressers,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"For the wheat and the barley;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Because the harvest of the field is destroyed."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"JOEL1:11","chapter":1,"book":"Joel","verse":11,"readable":"Joel 1:11"}},{"type":"verse","reference":{"id":"JOEL1:12","chapter":1,"book":"Joel","verse":12,"readable":"Joel 1:12"}},{"type":"char","content":"The vine has dried up"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the fig tree has withered;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"The pomegranate, the palm also, and the apple tree,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"All the trees of the field have dried up."}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Indeed, joy has dried up"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"From the sons of mankind."}]},{"type":"verse_end","reference":{"id":"JOEL1:12","chapter":1,"book":"Joel","verse":12,"readable":"Joel 1:12"}}]}]