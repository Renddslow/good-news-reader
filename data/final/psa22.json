[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"PS22:1","chapter":22,"book":"Ps","verse":1,"readable":"Psalms 22:1"}},{"type":"char","content":"My God, my God, why have You forsaken me?"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Far from my help are the words of my groaning."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:1","chapter":22,"book":"Ps","verse":1,"readable":"Psalms 22:1"}},{"type":"verse","reference":{"id":"PS22:2","chapter":22,"book":"Ps","verse":2,"readable":"Psalms 22:2"}},{"type":"char","content":"My God, I cry out by day, but You do not answer;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And by night, but I have no rest."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:2","chapter":22,"book":"Ps","verse":2,"readable":"Psalms 22:2"}},{"type":"verse","reference":{"id":"PS22:3","chapter":22,"book":"Ps","verse":3,"readable":"Psalms 22:3"}},{"type":"char","content":"Yet You are holy,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"You who are enthroned upon the praises of Israel."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:3","chapter":22,"book":"Ps","verse":3,"readable":"Psalms 22:3"}},{"type":"verse","reference":{"id":"PS22:4","chapter":22,"book":"Ps","verse":4,"readable":"Psalms 22:4"}},{"type":"char","content":"In You our fathers trusted;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"They trusted and You rescued them."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:4","chapter":22,"book":"Ps","verse":4,"readable":"Psalms 22:4"}},{"type":"verse","reference":{"id":"PS22:5","chapter":22,"book":"Ps","verse":5,"readable":"Psalms 22:5"}},{"type":"char","content":"To You they cried out and they fled to safety;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"In You they trusted and were not disappointed."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:5","chapter":22,"book":"Ps","verse":5,"readable":"Psalms 22:5"}},{"type":"verse","reference":{"id":"PS22:6","chapter":22,"book":"Ps","verse":6,"readable":"Psalms 22:6"}},{"type":"char","content":"But I am a worm and not a person,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"A disgrace of mankind and despised by the people."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:6","chapter":22,"book":"Ps","verse":6,"readable":"Psalms 22:6"}},{"type":"verse","reference":{"id":"PS22:7","chapter":22,"book":"Ps","verse":7,"readable":"Psalms 22:7"}},{"type":"char","content":"All who see me deride me;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"They sneer, they shake their heads, saying,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:7","chapter":22,"book":"Ps","verse":7,"readable":"Psalms 22:7"}},{"type":"verse","reference":{"id":"PS22:8","chapter":22,"book":"Ps","verse":8,"readable":"Psalms 22:8"}},{"type":"char","content":"“Turn him over to the Lord; let Him save him;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Let Him rescue him, because He delights in him.”"}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:8","chapter":22,"book":"Ps","verse":8,"readable":"Psalms 22:8"}},{"type":"verse","reference":{"id":"PS22:9","chapter":22,"book":"Ps","verse":9,"readable":"Psalms 22:9"}},{"type":"char","content":"Yet You are He who brought me forth from the womb;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"You made me trust when upon my mother’s breasts."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:9","chapter":22,"book":"Ps","verse":9,"readable":"Psalms 22:9"}},{"type":"verse","reference":{"id":"PS22:10","chapter":22,"book":"Ps","verse":10,"readable":"Psalms 22:10"}},{"type":"char","content":"I was cast upon You from birth;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"You have been my God from my mother’s womb."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:10","chapter":22,"book":"Ps","verse":10,"readable":"Psalms 22:10"}},{"type":"verse","reference":{"id":"PS22:11","chapter":22,"book":"Ps","verse":11,"readable":"Psalms 22:11"}},{"type":"char","content":"Do not be far from me, for trouble is near;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"For there is no one to help."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:11","chapter":22,"book":"Ps","verse":11,"readable":"Psalms 22:11"}},{"type":"verse","reference":{"id":"PS22:12","chapter":22,"book":"Ps","verse":12,"readable":"Psalms 22:12"}},{"type":"char","content":"Many bulls have surrounded me;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Strong bulls of Bashan have encircled me."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:12","chapter":22,"book":"Ps","verse":12,"readable":"Psalms 22:12"}},{"type":"verse","reference":{"id":"PS22:13","chapter":22,"book":"Ps","verse":13,"readable":"Psalms 22:13"}},{"type":"char","content":"They open their mouths wide at me,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"As a ravening and roaring lion."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:13","chapter":22,"book":"Ps","verse":13,"readable":"Psalms 22:13"}},{"type":"verse","reference":{"id":"PS22:14","chapter":22,"book":"Ps","verse":14,"readable":"Psalms 22:14"}},{"type":"char","content":"I am poured out like water,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And all my bones are out of joint;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"My heart is like wax;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"It is melted within me."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:14","chapter":22,"book":"Ps","verse":14,"readable":"Psalms 22:14"}},{"type":"verse","reference":{"id":"PS22:15","chapter":22,"book":"Ps","verse":15,"readable":"Psalms 22:15"}},{"type":"char","content":"My strength is dried up like a piece of pottery,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And my tongue clings to my jaws;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And You lay me in the dust of death."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:15","chapter":22,"book":"Ps","verse":15,"readable":"Psalms 22:15"}},{"type":"verse","reference":{"id":"PS22:16","chapter":22,"book":"Ps","verse":16,"readable":"Psalms 22:16"}},{"type":"char","content":"For dogs have surrounded me;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"A band of evildoers has encompassed me;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"They pierced my hands and my feet."}]},{"type":"poetry_line","indent":2,"children":[{"type":"verse_end","reference":{"id":"PS22:16","chapter":22,"book":"Ps","verse":16,"readable":"Psalms 22:16"}},{"type":"verse","reference":{"id":"PS22:17","chapter":22,"book":"Ps","verse":17,"readable":"Psalms 22:17"}},{"type":"char","content":"I can count all my bones."}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"They look, they stare at me;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:17","chapter":22,"book":"Ps","verse":17,"readable":"Psalms 22:17"}},{"type":"verse","reference":{"id":"PS22:18","chapter":22,"book":"Ps","verse":18,"readable":"Psalms 22:18"}},{"type":"char","content":"They divide my garments among them,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And they cast lots for my clothing."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:18","chapter":22,"book":"Ps","verse":18,"readable":"Psalms 22:18"}},{"type":"verse","reference":{"id":"PS22:19","chapter":22,"book":"Ps","verse":19,"readable":"Psalms 22:19"}},{"type":"char","content":"But You, Lord, do not be far away;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"You who are my help, hurry to my assistance."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:19","chapter":22,"book":"Ps","verse":19,"readable":"Psalms 22:19"}},{"type":"verse","reference":{"id":"PS22:20","chapter":22,"book":"Ps","verse":20,"readable":"Psalms 22:20"}},{"type":"char","content":"Save my soul from the sword,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"My only life from the power of the dog."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:20","chapter":22,"book":"Ps","verse":20,"readable":"Psalms 22:20"}},{"type":"verse","reference":{"id":"PS22:21","chapter":22,"book":"Ps","verse":21,"readable":"Psalms 22:21"}},{"type":"char","content":"Save me from the lion’s mouth;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"From the horns of the wild oxen You answer me."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:21","chapter":22,"book":"Ps","verse":21,"readable":"Psalms 22:21"}},{"type":"verse","reference":{"id":"PS22:22","chapter":22,"book":"Ps","verse":22,"readable":"Psalms 22:22"}},{"type":"char","content":"I will proclaim Your name to my brothers;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"In the midst of the assembly I will praise You."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:22","chapter":22,"book":"Ps","verse":22,"readable":"Psalms 22:22"}},{"type":"verse","reference":{"id":"PS22:23","chapter":22,"book":"Ps","verse":23,"readable":"Psalms 22:23"}},{"type":"char","content":"You who fear the Lord, praise Him;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"All you descendants of Jacob, glorify Him,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And stand in awe of Him, all you descendants of Israel."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:23","chapter":22,"book":"Ps","verse":23,"readable":"Psalms 22:23"}},{"type":"verse","reference":{"id":"PS22:24","chapter":22,"book":"Ps","verse":24,"readable":"Psalms 22:24"}},{"type":"char","content":"For He has not despised nor scorned the suffering of the afflicted;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Nor has He hidden His face from him;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"But when he cried to Him for help, He heard."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:24","chapter":22,"book":"Ps","verse":24,"readable":"Psalms 22:24"}},{"type":"verse","reference":{"id":"PS22:25","chapter":22,"book":"Ps","verse":25,"readable":"Psalms 22:25"}},{"type":"char","content":"From You comes my praise in the great assembly;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"I shall pay my vows before those who fear Him."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:25","chapter":22,"book":"Ps","verse":25,"readable":"Psalms 22:25"}},{"type":"verse","reference":{"id":"PS22:26","chapter":22,"book":"Ps","verse":26,"readable":"Psalms 22:26"}},{"type":"char","content":"The afflicted will eat and be satisfied;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Those who seek Him will praise the Lord."}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"May your heart live forever!"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:26","chapter":22,"book":"Ps","verse":26,"readable":"Psalms 22:26"}},{"type":"verse","reference":{"id":"PS22:27","chapter":22,"book":"Ps","verse":27,"readable":"Psalms 22:27"}},{"type":"char","content":"All the ends of the earth will remember and turn to the Lord,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And all the families of the nations will worship before You."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:27","chapter":22,"book":"Ps","verse":27,"readable":"Psalms 22:27"}},{"type":"verse","reference":{"id":"PS22:28","chapter":22,"book":"Ps","verse":28,"readable":"Psalms 22:28"}},{"type":"char","content":"For the kingdom is the Lord’s"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And He rules over the nations."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:28","chapter":22,"book":"Ps","verse":28,"readable":"Psalms 22:28"}},{"type":"verse","reference":{"id":"PS22:29","chapter":22,"book":"Ps","verse":29,"readable":"Psalms 22:29"}},{"type":"char","content":"All the prosperous of the earth will eat and worship,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"All those who go down to the dust will kneel before Him,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Even he who cannot keep his soul alive."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:29","chapter":22,"book":"Ps","verse":29,"readable":"Psalms 22:29"}},{"type":"verse","reference":{"id":"PS22:30","chapter":22,"book":"Ps","verse":30,"readable":"Psalms 22:30"}},{"type":"char","content":"A posterity will serve Him;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"It will be told of the Lord to the coming generation."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS22:30","chapter":22,"book":"Ps","verse":30,"readable":"Psalms 22:30"}},{"type":"verse","reference":{"id":"PS22:31","chapter":22,"book":"Ps","verse":31,"readable":"Psalms 22:31"}},{"type":"char","content":"They will come and will declare His righteousness"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"To a people who will be born, that He has performed it."}]},{"type":"verse_end","reference":{"id":"PS22:31","chapter":22,"book":"Ps","verse":31,"readable":"Psalms 22:31"}}]}]