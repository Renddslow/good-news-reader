[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"ISA55:1","chapter":55,"book":"Isa","verse":1,"readable":"Isaiah 55:1"}},{"type":"char","content":"“Come, all you who are thirsty,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"come to the waters;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and you who have no money,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"come, buy and eat!"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Come, buy wine and milk"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"without money and without cost."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:1","chapter":55,"book":"Isa","verse":1,"readable":"Isaiah 55:1"}},{"type":"verse","reference":{"id":"ISA55:2","chapter":55,"book":"Isa","verse":2,"readable":"Isaiah 55:2"}},{"type":"char","content":"Why spend money on what is not bread,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and your labor on what does not satisfy?"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Listen, listen to me, and eat what is good,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and you will delight in the richest of fare."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:2","chapter":55,"book":"Isa","verse":2,"readable":"Isaiah 55:2"}},{"type":"verse","reference":{"id":"ISA55:3","chapter":55,"book":"Isa","verse":3,"readable":"Isaiah 55:3"}},{"type":"char","content":"Give ear and come to me;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"listen, that you may"},{"id":"chay","type":"hyperlink","link":"chay","content":"live"},{"type":"char","content":"."}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"I will make an everlasting covenant with you,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"my faithful love promised to David."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:3","chapter":55,"book":"Isa","verse":3,"readable":"Isaiah 55:3"}},{"type":"verse","reference":{"id":"ISA55:4","chapter":55,"book":"Isa","verse":4,"readable":"Isaiah 55:4"}},{"type":"char","content":"See, I have made him"},{"id":"eid","type":"hyperlink","link":"eid","content":"a witness"},{"type":"char","content":" to the peoples,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"a ruler and commander of the peoples."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:4","chapter":55,"book":"Isa","verse":4,"readable":"Isaiah 55:4"}},{"type":"verse","reference":{"id":"ISA55:5","chapter":55,"book":"Isa","verse":5,"readable":"Isaiah 55:5"}},{"type":"char","content":"Surely you will summon nations you know not,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and nations you do not know will come running to you,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"because of YHWH your God,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"the Holy One of Israel,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"for he has endowed you with splendor.”"}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:5","chapter":55,"book":"Isa","verse":5,"readable":"Isaiah 55:5"}},{"type":"verse","reference":{"id":"ISA55:6","chapter":55,"book":"Isa","verse":6,"readable":"Isaiah 55:6"}},{"type":"char","content":"Seek YHWH while he may be found;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"call on him while he is near."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:6","chapter":55,"book":"Isa","verse":6,"readable":"Isaiah 55:6"}},{"type":"verse","reference":{"id":"ISA55:7","chapter":55,"book":"Isa","verse":7,"readable":"Isaiah 55:7"}},{"type":"char","content":"Let the wicked forsake their ways"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and the unrighteous their thoughts."}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Let them turn to YHWH, and he will have mercy on them,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and to our God, for he will freely pardon."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:7","chapter":55,"book":"Isa","verse":7,"readable":"Isaiah 55:7"}},{"type":"verse","reference":{"id":"ISA55:8","chapter":55,"book":"Isa","verse":8,"readable":"Isaiah 55:8"}},{"type":"char","content":"“For my thoughts are not your thoughts,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"neither are your ways my ways,”"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"declares YHWH."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:8","chapter":55,"book":"Isa","verse":8,"readable":"Isaiah 55:8"}},{"type":"verse","reference":{"id":"ISA55:9","chapter":55,"book":"Isa","verse":9,"readable":"Isaiah 55:9"}},{"type":"char","content":"“As the heavens are higher than the earth,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"so are my ways higher than your ways"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and my thoughts than your thoughts."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:9","chapter":55,"book":"Isa","verse":9,"readable":"Isaiah 55:9"}},{"type":"verse","reference":{"id":"ISA55:10","chapter":55,"book":"Isa","verse":10,"readable":"Isaiah 55:10"}},{"type":"char","content":"As the rain and the snow"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"come down from heaven,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and do not return to it"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"without watering the earth"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and making it bud and flourish,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"so that it yields seed for the sower and bread for the eater,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:10","chapter":55,"book":"Isa","verse":10,"readable":"Isaiah 55:10"}},{"type":"verse","reference":{"id":"ISA55:11","chapter":55,"book":"Isa","verse":11,"readable":"Isaiah 55:11"}},{"type":"char","content":"so is my word that goes out from my mouth:"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"It will not return to me empty,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"but will accomplish what I desire"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and achieve the purpose for which I sent it."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:11","chapter":55,"book":"Isa","verse":11,"readable":"Isaiah 55:11"}},{"type":"verse","reference":{"id":"ISA55:12","chapter":55,"book":"Isa","verse":12,"readable":"Isaiah 55:12"}},{"type":"char","content":"You will go out in joy"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and be led forth in peace;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"the mountains and hills"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"will burst into song before you,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and all the trees of the field"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"will clap their hands."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA55:12","chapter":55,"book":"Isa","verse":12,"readable":"Isaiah 55:12"}},{"type":"verse","reference":{"id":"ISA55:13","chapter":55,"book":"Isa","verse":13,"readable":"Isaiah 55:13"}},{"type":"char","content":"Instead of the thornbush will grow the juniper,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"and instead of briers the myrtle will grow."}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"This will be for YHWH’s renown,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"for an everlasting sign,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"that will endure forever.”"}]},{"type":"verse_end","reference":{"id":"ISA55:13","chapter":55,"book":"Isa","verse":13,"readable":"Isaiah 55:13"}}]}]