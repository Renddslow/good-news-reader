[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"PROV8:1","chapter":8,"book":"Prov","verse":1,"readable":"Proverbs 8:1"}},{"type":"char","content":"Does not wisdom call,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And understanding raise her voice?"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:1","chapter":8,"book":"Prov","verse":1,"readable":"Proverbs 8:1"}},{"type":"verse","reference":{"id":"PROV8:2","chapter":8,"book":"Prov","verse":2,"readable":"Proverbs 8:2"}},{"type":"char","content":"On top of the heights beside the way,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Where the paths meet, she takes her stand;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:2","chapter":8,"book":"Prov","verse":2,"readable":"Proverbs 8:2"}},{"type":"verse","reference":{"id":"PROV8:3","chapter":8,"book":"Prov","verse":3,"readable":"Proverbs 8:3"}},{"type":"char","content":"Beside the gates, at the opening to the city,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"At the entrance of the doors, she cries out:"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:3","chapter":8,"book":"Prov","verse":3,"readable":"Proverbs 8:3"}},{"type":"verse","reference":{"id":"PROV8:4","chapter":8,"book":"Prov","verse":4,"readable":"Proverbs 8:4"}},{"type":"char","content":"“To you, people, I call,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And my voice is to mankind."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:4","chapter":8,"book":"Prov","verse":4,"readable":"Proverbs 8:4"}},{"type":"verse","reference":{"id":"PROV8:5","chapter":8,"book":"Prov","verse":5,"readable":"Proverbs 8:5"}},{"type":"char","content":"You naive ones, understand prudence;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And, you fools, understand wisdom!"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:5","chapter":8,"book":"Prov","verse":5,"readable":"Proverbs 8:5"}},{"type":"verse","reference":{"id":"PROV8:6","chapter":8,"book":"Prov","verse":6,"readable":"Proverbs 8:6"}},{"type":"char","content":"Listen, for I will speak noble things;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And the opening of my lips will reveal right things."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:6","chapter":8,"book":"Prov","verse":6,"readable":"Proverbs 8:6"}},{"type":"verse","reference":{"id":"PROV8:7","chapter":8,"book":"Prov","verse":7,"readable":"Proverbs 8:7"}},{"type":"char","content":"For my mouth will proclaim truth;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And wickedness is an abomination to my lips."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:7","chapter":8,"book":"Prov","verse":7,"readable":"Proverbs 8:7"}},{"type":"verse","reference":{"id":"PROV8:8","chapter":8,"book":"Prov","verse":8,"readable":"Proverbs 8:8"}},{"type":"char","content":"All the words of my mouth are in righteousness;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"There is nothing crooked or perverted in them."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:8","chapter":8,"book":"Prov","verse":8,"readable":"Proverbs 8:8"}},{"type":"verse","reference":{"id":"PROV8:9","chapter":8,"book":"Prov","verse":9,"readable":"Proverbs 8:9"}},{"type":"char","content":"They are all straightforward to him who understands,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And right to those who find knowledge."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:9","chapter":8,"book":"Prov","verse":9,"readable":"Proverbs 8:9"}},{"type":"verse","reference":{"id":"PROV8:10","chapter":8,"book":"Prov","verse":10,"readable":"Proverbs 8:10"}},{"type":"char","content":"Accept my instruction and not silver,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And knowledge rather than choice gold."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:10","chapter":8,"book":"Prov","verse":10,"readable":"Proverbs 8:10"}},{"type":"verse","reference":{"id":"PROV8:11","chapter":8,"book":"Prov","verse":11,"readable":"Proverbs 8:11"}},{"type":"char","content":"For wisdom is better than jewels;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And all desirable things cannot compare with her."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:11","chapter":8,"book":"Prov","verse":11,"readable":"Proverbs 8:11"}},{"type":"verse","reference":{"id":"PROV8:12","chapter":8,"book":"Prov","verse":12,"readable":"Proverbs 8:12"}},{"type":"char","content":"“I, wisdom, dwell with prudence,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And I find knowledge and discretion."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:12","chapter":8,"book":"Prov","verse":12,"readable":"Proverbs 8:12"}},{"type":"verse","reference":{"id":"PROV8:13","chapter":8,"book":"Prov","verse":13,"readable":"Proverbs 8:13"}},{"type":"char","content":"The fear of the Lord is to hate evil;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Pride, arrogance, the evil way,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And the perverted mouth, I hate."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:13","chapter":8,"book":"Prov","verse":13,"readable":"Proverbs 8:13"}},{"type":"verse","reference":{"id":"PROV8:14","chapter":8,"book":"Prov","verse":14,"readable":"Proverbs 8:14"}},{"type":"char","content":"Advice is mine and sound wisdom;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"I am understanding, power is mine."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:14","chapter":8,"book":"Prov","verse":14,"readable":"Proverbs 8:14"}},{"type":"verse","reference":{"id":"PROV8:15","chapter":8,"book":"Prov","verse":15,"readable":"Proverbs 8:15"}},{"type":"char","content":"By me kings reign,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And rulers decree justice."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:15","chapter":8,"book":"Prov","verse":15,"readable":"Proverbs 8:15"}},{"type":"verse","reference":{"id":"PROV8:16","chapter":8,"book":"Prov","verse":16,"readable":"Proverbs 8:16"}},{"type":"char","content":"By me princes rule, and nobles,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"All who judge rightly."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:16","chapter":8,"book":"Prov","verse":16,"readable":"Proverbs 8:16"}},{"type":"verse","reference":{"id":"PROV8:17","chapter":8,"book":"Prov","verse":17,"readable":"Proverbs 8:17"}},{"type":"char","content":"I love those who love me;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And those who diligently seek me will find me."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:17","chapter":8,"book":"Prov","verse":17,"readable":"Proverbs 8:17"}},{"type":"verse","reference":{"id":"PROV8:18","chapter":8,"book":"Prov","verse":18,"readable":"Proverbs 8:18"}},{"type":"char","content":"Riches and honor are with me,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Enduring wealth and righteousness."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:18","chapter":8,"book":"Prov","verse":18,"readable":"Proverbs 8:18"}},{"type":"verse","reference":{"id":"PROV8:19","chapter":8,"book":"Prov","verse":19,"readable":"Proverbs 8:19"}},{"type":"char","content":"My fruit is better than gold, even pure gold;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And my yield better than choice silver."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:19","chapter":8,"book":"Prov","verse":19,"readable":"Proverbs 8:19"}},{"type":"verse","reference":{"id":"PROV8:20","chapter":8,"book":"Prov","verse":20,"readable":"Proverbs 8:20"}},{"type":"char","content":"I walk in the way of righteousness,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"In the midst of the paths of justice,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:20","chapter":8,"book":"Prov","verse":20,"readable":"Proverbs 8:20"}},{"type":"verse","reference":{"id":"PROV8:21","chapter":8,"book":"Prov","verse":21,"readable":"Proverbs 8:21"}},{"type":"char","content":"To endow those who love me with wealth,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"That I may fill their treasuries."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:21","chapter":8,"book":"Prov","verse":21,"readable":"Proverbs 8:21"}},{"type":"verse","reference":{"id":"PROV8:22","chapter":8,"book":"Prov","verse":22,"readable":"Proverbs 8:22"}},{"type":"char","content":"“The Lord created me at the beginning of His way,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Before His works of old."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:22","chapter":8,"book":"Prov","verse":22,"readable":"Proverbs 8:22"}},{"type":"verse","reference":{"id":"PROV8:23","chapter":8,"book":"Prov","verse":23,"readable":"Proverbs 8:23"}},{"type":"char","content":"From eternity I was established,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"From the beginning, from the earliest times of the earth."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:23","chapter":8,"book":"Prov","verse":23,"readable":"Proverbs 8:23"}},{"type":"verse","reference":{"id":"PROV8:24","chapter":8,"book":"Prov","verse":24,"readable":"Proverbs 8:24"}},{"type":"char","content":"When there were no ocean depths, I was born,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"When there were no springs abounding with water."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:24","chapter":8,"book":"Prov","verse":24,"readable":"Proverbs 8:24"}},{"type":"verse","reference":{"id":"PROV8:25","chapter":8,"book":"Prov","verse":25,"readable":"Proverbs 8:25"}},{"type":"char","content":"Before the mountains were settled,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Before the hills, I was born;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:25","chapter":8,"book":"Prov","verse":25,"readable":"Proverbs 8:25"}},{"type":"verse","reference":{"id":"PROV8:26","chapter":8,"book":"Prov","verse":26,"readable":"Proverbs 8:26"}},{"type":"char","content":"While He had not yet made the earth and the fields,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Nor the first dust of the world."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:26","chapter":8,"book":"Prov","verse":26,"readable":"Proverbs 8:26"}},{"type":"verse","reference":{"id":"PROV8:27","chapter":8,"book":"Prov","verse":27,"readable":"Proverbs 8:27"}},{"type":"char","content":"When He established the heavens, I was there;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"When He inscribed a circle on the face of the deep,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:27","chapter":8,"book":"Prov","verse":27,"readable":"Proverbs 8:27"}},{"type":"verse","reference":{"id":"PROV8:28","chapter":8,"book":"Prov","verse":28,"readable":"Proverbs 8:28"}},{"type":"char","content":"When He made firm the skies above,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"When the springs of the deep became fixed,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:28","chapter":8,"book":"Prov","verse":28,"readable":"Proverbs 8:28"}},{"type":"verse","reference":{"id":"PROV8:29","chapter":8,"book":"Prov","verse":29,"readable":"Proverbs 8:29"}},{"type":"char","content":"When He set a boundary for the sea"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"So that the water would not violate His command,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"When He marked out the foundations of the earth;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:29","chapter":8,"book":"Prov","verse":29,"readable":"Proverbs 8:29"}},{"type":"verse","reference":{"id":"PROV8:30","chapter":8,"book":"Prov","verse":30,"readable":"Proverbs 8:30"}},{"type":"char","content":"Then I was beside Him, as a master workman;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And I was His delight daily,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Rejoicing always before Him,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:30","chapter":8,"book":"Prov","verse":30,"readable":"Proverbs 8:30"}},{"type":"verse","reference":{"id":"PROV8:31","chapter":8,"book":"Prov","verse":31,"readable":"Proverbs 8:31"}},{"type":"char","content":"Rejoicing in the world, His earth,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And having my delight in the sons of mankind."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:31","chapter":8,"book":"Prov","verse":31,"readable":"Proverbs 8:31"}},{"type":"verse","reference":{"id":"PROV8:32","chapter":8,"book":"Prov","verse":32,"readable":"Proverbs 8:32"}},{"type":"char","content":"“Now then, sons, listen to me,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"For blessed are those who keep my ways."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:32","chapter":8,"book":"Prov","verse":32,"readable":"Proverbs 8:32"}},{"type":"verse","reference":{"id":"PROV8:33","chapter":8,"book":"Prov","verse":33,"readable":"Proverbs 8:33"}},{"type":"char","content":"Listen to instruction and be wise,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And do not neglect it."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:33","chapter":8,"book":"Prov","verse":33,"readable":"Proverbs 8:33"}},{"type":"verse","reference":{"id":"PROV8:34","chapter":8,"book":"Prov","verse":34,"readable":"Proverbs 8:34"}},{"type":"char","content":"Blessed is the person who listens to me,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Watching daily at my gates,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Waiting at my doorposts."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:34","chapter":8,"book":"Prov","verse":34,"readable":"Proverbs 8:34"}},{"type":"verse","reference":{"id":"PROV8:35","chapter":8,"book":"Prov","verse":35,"readable":"Proverbs 8:35"}},{"type":"char","content":"For one who finds me finds"},{"id":"chayyim","type":"hyperlink","link":"chayyim","content":"life"},{"type":"char","content":","}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And obtains favor from the Lord."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PROV8:35","chapter":8,"book":"Prov","verse":35,"readable":"Proverbs 8:35"}},{"type":"verse","reference":{"id":"PROV8:36","chapter":8,"book":"Prov","verse":36,"readable":"Proverbs 8:36"}},{"type":"char","content":"But one who sins against me injures himself;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"All those who hate me love death.”"}]},{"type":"verse_end","reference":{"id":"PROV8:36","chapter":8,"book":"Prov","verse":36,"readable":"Proverbs 8:36"}}]}]