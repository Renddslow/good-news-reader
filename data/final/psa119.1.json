[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"PS119:1","chapter":119,"book":"Ps","verse":1,"readable":"Psalms 119:1"}},{"type":"char","content":"Blessed are those whose way is blameless,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Who walk in the Law of the Lord."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS119:1","chapter":119,"book":"Ps","verse":1,"readable":"Psalms 119:1"}},{"type":"verse","reference":{"id":"PS119:2","chapter":119,"book":"Ps","verse":2,"readable":"Psalms 119:2"}},{"type":"char","content":"Blessed are those who comply with His testimonies,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And seek Him with all their heart."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS119:2","chapter":119,"book":"Ps","verse":2,"readable":"Psalms 119:2"}},{"type":"verse","reference":{"id":"PS119:3","chapter":119,"book":"Ps","verse":3,"readable":"Psalms 119:3"}},{"type":"char","content":"They also do no injustice;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"They walk in His ways."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS119:3","chapter":119,"book":"Ps","verse":3,"readable":"Psalms 119:3"}},{"type":"verse","reference":{"id":"PS119:4","chapter":119,"book":"Ps","verse":4,"readable":"Psalms 119:4"}},{"type":"char","content":"You have ordained Your precepts,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"That we are to keep them diligently."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS119:4","chapter":119,"book":"Ps","verse":4,"readable":"Psalms 119:4"}},{"type":"verse","reference":{"id":"PS119:5","chapter":119,"book":"Ps","verse":5,"readable":"Psalms 119:5"}},{"type":"char","content":"Oh that my ways may be established"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"To keep Your statutes!"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS119:5","chapter":119,"book":"Ps","verse":5,"readable":"Psalms 119:5"}},{"type":"verse","reference":{"id":"PS119:6","chapter":119,"book":"Ps","verse":6,"readable":"Psalms 119:6"}},{"type":"char","content":"Then I will not be ashamed"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"When I look at all Your commandments."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS119:6","chapter":119,"book":"Ps","verse":6,"readable":"Psalms 119:6"}},{"type":"verse","reference":{"id":"PS119:7","chapter":119,"book":"Ps","verse":7,"readable":"Psalms 119:7"}},{"type":"char","content":"I will give thanks to You with uprightness of heart,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"When I learn Your righteous judgments."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS119:7","chapter":119,"book":"Ps","verse":7,"readable":"Psalms 119:7"}},{"type":"verse","reference":{"id":"PS119:8","chapter":119,"book":"Ps","verse":8,"readable":"Psalms 119:8"}},{"type":"char","content":"I will keep Your statutes;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Do not utterly abandon me!"}]},{"type":"verse_end","reference":{"id":"PS119:8","chapter":119,"book":"Ps","verse":8,"readable":"Psalms 119:8"}}]}]