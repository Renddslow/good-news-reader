[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"ISA49:1","chapter":49,"book":"Isa","verse":1,"readable":"Isaiah 49:1"}},{"type":"char","content":"Listen to Me, you islands,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And pay attention, you peoples from afar."}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"The Lord called Me from the womb;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"From the body of My mother He named Me."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA49:1","chapter":49,"book":"Isa","verse":1,"readable":"Isaiah 49:1"}},{"type":"verse","reference":{"id":"ISA49:2","chapter":49,"book":"Isa","verse":2,"readable":"Isaiah 49:2"}},{"type":"char","content":"He has made My mouth like a sharp sword,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"In the shadow of His hand He has concealed Me;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And He has also made Me a sharpened arrow,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"He has hidden Me in His quiver."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA49:2","chapter":49,"book":"Isa","verse":2,"readable":"Isaiah 49:2"}},{"type":"verse","reference":{"id":"ISA49:3","chapter":49,"book":"Isa","verse":3,"readable":"Isaiah 49:3"}},{"type":"char","content":"He said to Me, “You are My Servant, Israel,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"In whom I will show My glory.”"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"ISA49:3","chapter":49,"book":"Isa","verse":3,"readable":"Isaiah 49:3"}},{"type":"verse","reference":{"id":"ISA49:4","chapter":49,"book":"Isa","verse":4,"readable":"Isaiah 49:4"}},{"type":"char","content":"But I said, “I have labored in vain,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"I have spent My strength for nothing and futility;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Nevertheless, the justice due to Me is with the Lord,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And My reward is with My God.”"}]},{"type":"verse_end","reference":{"id":"ISA49:4","chapter":49,"book":"Isa","verse":4,"readable":"Isaiah 49:4"}}]}]