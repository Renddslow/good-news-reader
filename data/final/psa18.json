[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"PS18:1","chapter":18,"book":"Ps","verse":1,"readable":"Psalms 18:1"}},{"type":"char","content":"“I love You, Lord, my strength.”"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:1","chapter":18,"book":"Ps","verse":1,"readable":"Psalms 18:1"}},{"type":"verse","reference":{"id":"PS18:2","chapter":18,"book":"Ps","verse":2,"readable":"Psalms 18:2"}},{"type":"char","content":"The Lord is my rock and my fortress and my savior,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"My God, my rock, in whom I take refuge;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"My shield and the horn of my salvation, my stronghold."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:2","chapter":18,"book":"Ps","verse":2,"readable":"Psalms 18:2"}},{"type":"verse","reference":{"id":"PS18:3","chapter":18,"book":"Ps","verse":3,"readable":"Psalms 18:3"}},{"type":"char","content":"I call upon the Lord, who is worthy to be praised,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And I am saved from my enemies."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:3","chapter":18,"book":"Ps","verse":3,"readable":"Psalms 18:3"}},{"type":"verse","reference":{"id":"PS18:4","chapter":18,"book":"Ps","verse":4,"readable":"Psalms 18:4"}},{"type":"char","content":"The ropes of death encompassed me,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the torrents of destruction terrified me."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:4","chapter":18,"book":"Ps","verse":4,"readable":"Psalms 18:4"}},{"type":"verse","reference":{"id":"PS18:5","chapter":18,"book":"Ps","verse":5,"readable":"Psalms 18:5"}},{"type":"char","content":"The ropes of Sheol surrounded me;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"The snares of death confronted me."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:5","chapter":18,"book":"Ps","verse":5,"readable":"Psalms 18:5"}},{"type":"verse","reference":{"id":"PS18:6","chapter":18,"book":"Ps","verse":6,"readable":"Psalms 18:6"}},{"type":"char","content":"In my distress I called upon the Lord,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And cried to my God for help;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"He heard my voice from His temple,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And my cry for help before Him came into His ears."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:6","chapter":18,"book":"Ps","verse":6,"readable":"Psalms 18:6"}},{"type":"verse","reference":{"id":"PS18:7","chapter":18,"book":"Ps","verse":7,"readable":"Psalms 18:7"}},{"type":"char","content":"Then the earth shook and quaked;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the foundations of the mountains were trembling"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And were shaken, because He was angry."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:7","chapter":18,"book":"Ps","verse":7,"readable":"Psalms 18:7"}},{"type":"verse","reference":{"id":"PS18:8","chapter":18,"book":"Ps","verse":8,"readable":"Psalms 18:8"}},{"type":"char","content":"Smoke went up out of His nostrils,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And fire from His mouth was devouring;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Coals burned from it."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:8","chapter":18,"book":"Ps","verse":8,"readable":"Psalms 18:8"}},{"type":"verse","reference":{"id":"PS18:9","chapter":18,"book":"Ps","verse":9,"readable":"Psalms 18:9"}},{"type":"char","content":"He also bowed the heavens down low, and came down"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"With thick darkness under His feet."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:9","chapter":18,"book":"Ps","verse":9,"readable":"Psalms 18:9"}},{"type":"verse","reference":{"id":"PS18:10","chapter":18,"book":"Ps","verse":10,"readable":"Psalms 18:10"}},{"type":"char","content":"He rode on a cherub and flew;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And He sped on the wings of the wind."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:10","chapter":18,"book":"Ps","verse":10,"readable":"Psalms 18:10"}},{"type":"verse","reference":{"id":"PS18:11","chapter":18,"book":"Ps","verse":11,"readable":"Psalms 18:11"}},{"type":"char","content":"He made darkness His hiding place, His canopy around Him,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Darkness of waters, thick clouds."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:11","chapter":18,"book":"Ps","verse":11,"readable":"Psalms 18:11"}},{"type":"verse","reference":{"id":"PS18:12","chapter":18,"book":"Ps","verse":12,"readable":"Psalms 18:12"}},{"type":"char","content":"From the brightness before Him passed His thick clouds,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Hailstones and coals of fire."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:12","chapter":18,"book":"Ps","verse":12,"readable":"Psalms 18:12"}},{"type":"verse","reference":{"id":"PS18:13","chapter":18,"book":"Ps","verse":13,"readable":"Psalms 18:13"}},{"type":"char","content":"The Lord also thundered in the heavens,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the Most High uttered His voice,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Hailstones and coals of fire."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:13","chapter":18,"book":"Ps","verse":13,"readable":"Psalms 18:13"}},{"type":"verse","reference":{"id":"PS18:14","chapter":18,"book":"Ps","verse":14,"readable":"Psalms 18:14"}},{"type":"char","content":"He sent out His arrows, and scattered them,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And lightning flashes in abundance, and routed them."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS18:14","chapter":18,"book":"Ps","verse":14,"readable":"Psalms 18:14"}},{"type":"verse","reference":{"id":"PS18:15","chapter":18,"book":"Ps","verse":15,"readable":"Psalms 18:15"}},{"type":"char","content":"Then the channels of water appeared,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the foundations of the world were exposed"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"By Your rebuke, Lord,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"At the blast of the breath of Your nostrils."}]},{"type":"verse_end","reference":{"id":"PS18:15","chapter":18,"book":"Ps","verse":15,"readable":"Psalms 18:15"}}]}]