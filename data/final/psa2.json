[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"PS2:1","chapter":2,"book":"Ps","verse":1,"readable":"Psalms 2:1"}},{"type":"char","content":"Why are the nations restless"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the peoples plotting in vain?"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:1","chapter":2,"book":"Ps","verse":1,"readable":"Psalms 2:1"}},{"type":"verse","reference":{"id":"PS2:2","chapter":2,"book":"Ps","verse":2,"readable":"Psalms 2:2"}},{"type":"char","content":"The kings of the earth take their stand"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the rulers conspire together"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Against the Lord and against His Anointed, saying,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"verse_end","reference":{"id":"PS2:2","chapter":2,"book":"Ps","verse":2,"readable":"Psalms 2:2"}},{"type":"verse","reference":{"id":"PS2:3","chapter":2,"book":"Ps","verse":3,"readable":"Psalms 2:3"}},{"type":"char","content":"“Let’s tear their shackles apart"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And throw their ropes away from us!”"}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:3","chapter":2,"book":"Ps","verse":3,"readable":"Psalms 2:3"}},{"type":"verse","reference":{"id":"PS2:4","chapter":2,"book":"Ps","verse":4,"readable":"Psalms 2:4"}},{"type":"char","content":"He who sits in the heavens laughs,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"The Lord scoffs at them."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:4","chapter":2,"book":"Ps","verse":4,"readable":"Psalms 2:4"}},{"type":"verse","reference":{"id":"PS2:5","chapter":2,"book":"Ps","verse":5,"readable":"Psalms 2:5"}},{"type":"char","content":"Then He will speak to them in His anger"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And terrify them in His fury, saying,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:5","chapter":2,"book":"Ps","verse":5,"readable":"Psalms 2:5"}},{"type":"verse","reference":{"id":"PS2:6","chapter":2,"book":"Ps","verse":6,"readable":"Psalms 2:6"}},{"type":"char","content":"“But as for Me, I have installed My King"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Upon Zion, My holy mountain.”"}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:6","chapter":2,"book":"Ps","verse":6,"readable":"Psalms 2:6"}},{"type":"verse","reference":{"id":"PS2:7","chapter":2,"book":"Ps","verse":7,"readable":"Psalms 2:7"}},{"type":"char","content":"“I will announce the decree of the Lord:"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"He said to Me, ‘You are My Son,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Today I have fathered You."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:7","chapter":2,"book":"Ps","verse":7,"readable":"Psalms 2:7"}},{"type":"verse","reference":{"id":"PS2:8","chapter":2,"book":"Ps","verse":8,"readable":"Psalms 2:8"}},{"type":"char","content":"Ask it of Me, and I will certainly give the nations as Your inheritance,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And the ends of the earth as Your possession."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:8","chapter":2,"book":"Ps","verse":8,"readable":"Psalms 2:8"}},{"type":"verse","reference":{"id":"PS2:9","chapter":2,"book":"Ps","verse":9,"readable":"Psalms 2:9"}},{"type":"char","content":"You shall break them with a rod of iron,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"You shall shatter them like earthenware.’”"}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:9","chapter":2,"book":"Ps","verse":9,"readable":"Psalms 2:9"}},{"type":"verse","reference":{"id":"PS2:10","chapter":2,"book":"Ps","verse":10,"readable":"Psalms 2:10"}},{"type":"char","content":"Now then, you kings, use insight;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Let yourselves be instructed, you judges of the earth."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:10","chapter":2,"book":"Ps","verse":10,"readable":"Psalms 2:10"}},{"type":"verse","reference":{"id":"PS2:11","chapter":2,"book":"Ps","verse":11,"readable":"Psalms 2:11"}},{"type":"char","content":"Serve the Lord with reverence"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And rejoice with trembling."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS2:11","chapter":2,"book":"Ps","verse":11,"readable":"Psalms 2:11"}},{"type":"verse","reference":{"id":"PS2:12","chapter":2,"book":"Ps","verse":12,"readable":"Psalms 2:12"}},{"type":"char","content":"Kiss the Son, that He not be angry and you perish on the way,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"For His wrath may be kindled quickly."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"How blessed are all who take refuge in Him!"}]},{"type":"verse_end","reference":{"id":"PS2:12","chapter":2,"book":"Ps","verse":12,"readable":"Psalms 2:12"}}]}]