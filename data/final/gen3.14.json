[{"type":"paragraph","children":[{"type":"verse","reference":{"id":"GEN3:14","chapter":3,"book":"Gen","verse":14,"readable":"Genesis 3:14"}},{"type":"char","content":"Then the Lord God said to the serpent,"}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"“Because you have done this,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Cursed are you more than all the livestock,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And more than any animal of the field;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"On your belly you shall go,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And dust you shall eat"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"All the days of your life;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"GEN3:14","chapter":3,"book":"Gen","verse":14,"readable":"Genesis 3:14"}},{"type":"verse","reference":{"id":"GEN3:15","chapter":3,"book":"Gen","verse":15,"readable":"Genesis 3:15"}},{"type":"char","content":"And I will make enemies"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Of you and the woman,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And of your offspring and her Descendant;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"He shall bruise you on the head,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And you shall bruise Him on the heel.”"}]}]},{"type":"paragraph","children":[{"type":"verse_end","reference":{"id":"GEN3:15","chapter":3,"book":"Gen","verse":15,"readable":"Genesis 3:15"}},{"type":"verse","reference":{"id":"GEN3:16","chapter":3,"book":"Gen","verse":16,"readable":"Genesis 3:16"}},{"type":"char","content":"To the woman He said,"}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"“I will greatly multiply"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Your pain in childbirth,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"In pain you shall deliver children;"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"Yet your desire will be for your husband,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And he shall rule over you.”"}]},{"type":"verse_end","reference":{"id":"GEN3:16","chapter":3,"book":"Gen","verse":16,"readable":"Genesis 3:16"}}]}]